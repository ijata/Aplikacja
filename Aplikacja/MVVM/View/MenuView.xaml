<UserControl x:Class="Aplikacja.MVVM.View.MenuView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Aplikacja.MVVM.View" xmlns:viewmodel="clr-namespace:Aplikacja.MVVM.ViewModel" d:DataContext="{d:DesignInstance Type=viewmodel:MenuViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <ScrollViewer VerticalScrollBarVisibility="Auto">
            <TextBlock Foreground="White" Text="{Binding menuSeven}" TextWrapping="Wrap"   VerticalAlignment="Top" HorizontalAlignment="Left" Margin="20,0,0,0"/>
        </ScrollViewer>
    </Grid>
</UserControl>
